{
  "name": "netcup-node",
  "version": "0.0.5",
  "description": "A node wrapper for the Netcup CCP API (https://www.netcup-wiki.de/wiki/CCP_API)",
  "author": "proohit <direnc99@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proohit/netcup-node.git"
  },
  "bugs": {
    "url": "https://github.com/proohit/netcup-node/issues"
  },
  "keywords": [
    "node",
    "netcup",
    "ccp"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "scripts": {
    "start": "node lib/index.js",
    "clean": "rimraf coverage build lib tmp",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.release.json && tsc-esm-fix --target=lib/",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prerelease": "npm run build",
    "release": "npm login && bump --tag && npm publish",
    "postrelease": "git push && git push --tags"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "tslib": "~2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "tsc-esm-fix": "^2.13.0",
    "tsutils": "^3.21.0",
    "typescript": "^4.6.3",
    "version-bump-prompt": "^6.1.0"
  },
  "engines": {
    "node": ">= 12.20 <15"
  },
  "volta": {
    "node": "12.20.0"
  }
}
